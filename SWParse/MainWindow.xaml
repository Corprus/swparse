<Window x:Class="SWParse.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="652.2">
    <DockPanel Margin="17,10,3.4,11.4" RenderTransformOrigin="0.5,0.5">
        <Button Content="Parse Log" HorizontalAlignment="Left" Margin="10,0,0,0" Width="75" Click="Button_Click_1" Height="22" DockPanel.Dock="Top" />
        <RichTextBox x:Name="logText" Width="604" Margin="0,4.6,0,-0.2">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
    </DockPanel>

</Window>
